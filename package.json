{
  "name": "await-for-it",
  "version": "0.1.0-alpha04",
  "description": "Handy async iterables like pool, queue, merge, chunk, flatten, map, filter, etc.",
  "main": "src/index.js",
  "module": "src/chainable.js",
  "files": [
    "src",
    "umd"
  ],
  "scripts": {
    "build": "npm-run-all --serial test lint bundle test:package test:browser",
    "bundle": "npm-run-all --serial bundle:clean bundle:umd bundle:test",
    "bundle:clean": "rimraf umd temp",
    "bundle:combinetests": "node -r esm scripts/combine_all_tests.js",
    "bundle:test": "npm-run-all --serial bundle:combinetests bundle:umdtests",
    "bundle:umd": "rollup -c rollup.config.js",
    "bundle:umdtests": "rollup -c scripts/rollup.tests.config.js",
    "lint": "eslint src test",
    "test": "tape -r esm test/*_test.js | tap-nirvana",
    "test:browser": "node -r esm scripts/cat.js temp/all_test_umd.js | tape-run | tap-nirvana",
    "test:package": "node -r esm scripts/package_test.js"
  },
  "eslintConfig": {
    "extends": "@toolbuilder/eslint-config"
  },
  "dependencies": {
    "@toolbuilder/isnumber": "^0.1.1",
    "@toolbuilder/make-factory": "^0.1.1",
    "esm": "^3.2.25",
    "iterablefu": "^0.4.1"
  },
  "devDependencies": {
    "@toolbuilder/eslint-config": "^0.1.2",
    "@toolbuilder/list": "^1.0.0",
    "documentation": "^12.3.0",
    "eslint": "^6.8.0",
    "markedpp": "^1.0.1",
    "multistream": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "shelljs": "^0.8.3",
    "sort-package-json": "^1.41.0",
    "tap-nirvana": "^1.1.0",
    "tape": "^4.13.2",
    "zora": "^3.1.8"
  }
}
